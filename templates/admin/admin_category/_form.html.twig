{{ form_start(form) }}
    {% for field in form %}
        {% if field.vars.name != '_token' %}
            <div class="form-group">
                {{ form_label(field) }}
                {{ form_widget(field) }}
            </div>
        {% endif %}
    {% endfor %}
    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
